<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:p="http://www.springframework.org/schema/p"
        xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.1.xsd">
        
        
        <context:annotation-config/>
        
        <bean id="address1" class="com.cn.qualifire.annotation.Address" p:id="101" p:name="Chandan"/>
        <bean id="address2" class="com.cn.qualifire.annotation.Address" p:id="102" p:name="Chiku"/>
        
        <bean id="student" class="com.cn.qualifire.annotation.Student"/>
       
        
        
</beans>